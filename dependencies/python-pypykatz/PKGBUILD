pkgname=python-pypykatz
_pkgname=pypykatz
pkgver=0.3.8
pkgrel=1
pkgdesc='Mimikatz implementation in pure Python'
arch=(any)
url='https://github.com/skelsec/pypykatz'
license=('MIT')
depends=('python')
makedepends=('python-setuptools')
source=("https://files.pythonhosted.org/packages/13/f2/ac72f06d13734e568b4e555f2d17037dc07810001ea4bac66d6dac5c5172/pypykatz-0.3.8.tar.gz")
md5sums=('SKIP')

build() {
  cd "$srcdir/$_pkgname-$pkgver"
  python setup.py build
}

package() {
  cd "$srcdir/$_pkgname-$pkgver"
  python setup.py install --skip-build -O1 --root="$pkgdir"
}
